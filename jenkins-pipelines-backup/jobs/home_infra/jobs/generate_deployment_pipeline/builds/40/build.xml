    <script>pipeline {
    agent { label &apos;slave&apos; }

    environment {
        INVENTORY_FILE = &apos;ansible/inventories/hosts.yml&apos;
        PLAYBOOKS_DIR  = &apos;ansible/playbooks&apos;
